          <AgentSection mode="Survival">
            <Name>MalmoTutorialBot</Name><!-- Our AI's name -->
        <AgentStart>
            <Placement x="5.5" y="4" z="-9.5" yaw="0"/><!-- Start position of our agent. Z is NS  -->
            <Inventory>
                <InventoryItem slot="0" type="diamond_pickaxe"/>  <!-- Default weapon -->
            </Inventory>
        </AgentStart>
        <AgentHandlers>
          <ObservationFromFullStats/>
            <VideoProducer want_depth="false">
                <Width>700</Width>
                <Height>700</Height>
            </VideoProducer>
            <ObservationFromGrid>
              <Grid name="floor3x3">
                <min x="-1" y="-1" z="-1"/>
                <max x="1" y="-1" z="1"/>
              </Grid>
            </ObservationFromGrid>

            <DiscreteMovementCommands>
              <ModifierList type="deny-list">
              <command>attack</command>
              </ModifierList>
            </DiscreteMovementCommands>

            <RewardForTouchingBlockType>
              <Block reward="-100.0" type="lava" behaviour="onceOnly"/>
              <Block reward="5.0" type="lapis_block" behaviour="onceOnly"/>
            </RewardForTouchingBlockType>
            
            <RewardForSendingCommand reward="-1"/>

            <ContinuousMovementCommands turnSpeedDegs="180"/>
              <AgentQuitFromTouchingBlockType>
              <Block type="diamond_block"/>
                  </AgentQuitFromTouchingBlockType>
                </AgentHandlers>
              </AgentSection>
            </Mission>